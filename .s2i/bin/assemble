#!/bin/sh
# Called during the image build life cycle.

CONFIG="/tmp/src/config"
LIB="/tmp/src/lib"

if [ -d $CONFIG ]; then
    echo "Copying config files..."
    cp -rfv $CONFIG/* $AMQ_HOME/conf/
    echo "Done"
fi

view_and_admin_groups=admin,view
admin_groups=admin

export view_and_admin_groups
export admin_groups

echo $view_and_admin_groups
echo $admin_groups
